<!DOCTYPE HTML>
<html lang="en">
<head>
<meta charset="utf-8">
<title>Splash</title>
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<link href="stylesheets/bootstrap-responsive.css" rel="stylesheet">
<link href="stylesheets/bootstrap.min.css" rel="stylesheet">
<link href="stylesheets/style.css" rel="stylesheet">
<link href="stylesheets/default.css" rel="stylesheet">
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU" crossorigin="anonymous">
</head>

<body  style="background:	#C8C8C8;">
<!-- navbar -->
<div class="navbar-wrapper">
	<div class="navbar navbar-inverse navbar-fixed-top">
		<div class="navbar-inner">
			<div class="container">
				<!-- Responsive navbar -->
				<a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse"><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
				</a>
				<h1 class="brand"><a href="/">Splash</a></h1>
				<!-- navigation -->
				<nav class="pull-right nav-collapse collapse nav team">
				<ul id="menu-main" class="nav">
					<li><a title="team" >Welcome!!</a></li>
				</ul>
				</nav>
			</div>
		</div>
	</div>
</div><br><br>


  <div class="container">
    <div class="row">
      <!-- <div class="span3 home-post"><br>
        <ul class="widget widget-menu unstyled" style="margin-left:20px;">
                  <li><a href=""><i class="menu-icon icon-paste"></i>&nbsp New Post </a></li><br>
                  <li><a href=""><i class="menu-icon icon-table"></i> &nbsp All Posts </a></li>
          </ul>
      </div> -->

      <div class="span9 home-post">
        <div class="content">

          <div class="module">
            <div class="module-head"><br>
              <h3 style="text-align: center;">Manage your posts.</h3><hr>
            </div>
            <div class="module-body">

              <table class="table table-striped">
                <thead>
	                <tr>
	                  <th>#</th>
	                  <th>Title</th>
	                  <th>Description</th>
	                  <th>Picture 1</th>
	                  <th></th>
	                </tr>
	              </thead>
	              <tbody>
									<% authorStories.forEach(function(aStory,i){ %>
		                <tr>
		                  <td><%= ++i %></td>
		                  <td><%= aStory.title %></td>
		                  <td><%= aStory.story %></td>
		                  <td>no picture</td>
		                  <td>
		                    <a href="/editStory/<%= aStory._id %>" class="btn btn-primary btn-xs">Edit</a>
		                    <a href="/deleteStory/<%= aStory._id %>" class="btn btn-danger btn-xs">Delete</a>
		                  </td>
		                </tr>
									<% }) %>
                </tbody>
              </table>
            </div>
          </div>


        </div><!--/.content-->
      </div><!--/.span9-->
    </div>
  </div><!--/.container-->


</body>
</html>
